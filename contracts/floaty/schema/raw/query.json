{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "description": "Returns a list of all instructions",
      "type": "object",
      "required": [
        "instructions"
      ],
      "properties": {
        "instructions": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Performs a huge amount of floating point operations and hashes them together",
      "type": "object",
      "required": [
        "run"
      ],
      "properties": {
        "run": {
          "type": "object",
          "required": [
            "instruction",
            "seed"
          ],
          "properties": {
            "instruction": {
              "type": "string"
            },
            "seed": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  ]
}
